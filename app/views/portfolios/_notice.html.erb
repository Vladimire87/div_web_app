<% flash.each do |type, msg| %>
  <div class="alert alert-success alert-dismissible fade show" role="alert">
    <strong w-tid="48"><i class="bi bi-check-circle" w-tid="49"></i> <%= type.capitalize %></strong> <%= msg %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" w-tid="50"></button>
  </div>
<% end %>
